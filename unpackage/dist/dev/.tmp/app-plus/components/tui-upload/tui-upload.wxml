<view class="tui-container"><view class="tui-upload-box"><block wx:for="{{imageList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="tui-image-item"><image class="tui-item-img" src="{{item}}" mode="aspectFill" data-event-opts="{{[['tap',[['previewImage',[index]]]]]}}" catchtap="__e"></image><block wx:if="{{!forbidDel}}"><view data-event-opts="{{[['tap',[['delImage',[index]]]]]}}" class="tui-img-del" catchtap="__e"></view></block><block wx:if="{{statusArr[index]!=1}}"><view class="tui-upload-mask"><block wx:if="{{statusArr[index]==2}}"><view class="tui-upload-loading"></view></block><text class="tui-tips">{{statusArr[index]==2?'上传中...':'上传失败'}}</text><block wx:if="{{statusArr[index]==3}}"><view class="tui-mask-btn" hover-class="tui-hover" hover-stay-time="{{150}}" data-event-opts="{{[['tap',[['reUpLoad',[index]]]]]}}" catchtap="__e">重新上传</view></block></view></block></view></block><block wx:if="{{isShowAdd}}"><view data-event-opts="{{[['tap',[['chooseImage',['$event']]]]]}}" class="tui-upload-add" bindtap="__e"><view class="tui-upload-icon tui-icon-plus"></view></view></block></view></view>