	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
}
.tui-list-cell-navigate::after {
	content: '';
	position: absolute;
	border-bottom: 1rpx solid #eaeef1;
	-webkit-transform: scaleY(0.5);
	-ms-transform: scaleY(0.5);
	    transform: scaleY(0.5);
	bottom: 0;
	right: 0;
	left: 30rpx;
}
