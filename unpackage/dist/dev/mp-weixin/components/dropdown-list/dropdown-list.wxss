
.tui-dropdown-list {
	position: relative;
}
.tui-dropdown-view {
	width: 100%;
	overflow: hidden;
	position: absolute;
	z-index: 9999;
	left: 0;
	/* opacity: 0; */
	visibility: hidden;
	-webkit-transition: all 0.2s ease-in-out;
	-o-transition: all 0.2s ease-in-out;
	transition: all 0.2s ease-in-out;
}
.tui-dropdownlist-show {
	/* opacity: 1; */
	visibility: visible;
}

